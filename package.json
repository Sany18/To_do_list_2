{
  "name": "rct",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "proxy": "http://localhost:3000",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "express": "^4.16.4",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-scripts": "^2.1.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "start": "PORT=4000 react-scripts start",
    "build": "react-scripts build",
    "test": "clear && cat ./__tests__/jimg && jest --verbose --env=jsdom",
    "cov": "clear && cat ./__tests__/jimg && jest --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/babel-jest"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|ttf|css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ]
  },
  "devDependencies": {
    "acorn": "^6.0.3",
    "acorn-jsx": "^5.0.0",
    "ajv": "^6.5.5",
    "babel-core": "^6.7.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jquery": "^3.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.0",
    "uglify-js-brunch": "^2.10.0"
  }
}
